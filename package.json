{
  "name": "Macaulay2Web",
  "version": "3.3.0",
  "description": "Web app for Macaulay2",
  "contributors": [
    {
      "name": "Franziska Hinkelmann",
      "email": "franziska.hinkelmann@gmail.com"
    },
    {
      "name": "Lars Kastner",
      "email": "kastner@math.fu-berlin.de"
    },
    {
      "name": "Mike Stillman",
      "email": "mikestillman1@gmail.com"
    },
    {
      "name": "Paul Zinn-Justin",
      "email": "pzinn@unimelb.edu.au"
    }
  ],
  "engine": {
    "node": ">=10.0.0"
  },
  "sideEffects": false,
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.19",
    "@types/socket.io": "^2.1.12",
    "@types/socket.io-client": "^1.4.34",
    "@types/ssh2": "^0.5.46",
    "cookie": "^0.4.1",
    "express": "4.16.1",
    "express-winston": "3.1.0",
    "http-auth": "3.2.3",
    "prismjs": "^1.23.0",
    "serve-favicon": "^2.5.0",
    "serve-index": "^1.9.1",
    "serve-static": "^1.14.1",
    "socket.io": "2.2.0",
    "socket.io-client": "2.2.0",
    "socketio-file-upload": "^0.7.3",
    "ssh2": "0.8.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "winston": "^3.3.2"
  },
  "scripts": {
    "postinstall": "./postinstall",
    "tsc": "./prebuild && tsc && tsc -p src/frontend/",
    "build": "npm run lint && npm run format && npm run tsc && ./node_modules/.bin/webpack --env production",
    "build:minimal": "npm run tsc && ./node_modules/.bin/webpack --env minimal",
    "build:debug": "npm run tsc && ./node_modules/.bin/webpack --env debug",
    "css": "cd src/frontend/css; lessc ../../../KaTeX/src/katex.less katex.css; cat katex.css VectorGraphics.css interface_common.css interface_minimal.css prism-M2.css | postcss -o ../../../public/minimal.css && cat material.indigo-deep_orange.min.css katex.css VectorGraphics.css interface_common.css interface_extra.css prism-M2.css | postcss -o ../../../public/index.css",
    "start": "node dist/server/index.js",
    "docker": "npm run build && npm start docker",
    "local": "npm run build && npm start local",
    "debug": "npm run build:debug && npm start local",
    "twoMachines": "npm run build && npm start ssh",
    "twoMachines_forever": "npm run build && forever start -l $HOME/webm2.logs/forever.log -o $HOME/webm2.logs/out.log -e $HOME/webm2.logs/err.log --append dist/index.js ssh",
    "stop": "kill `ps ax | grep Server | grep -v monitor | grep node | awk '{print $1}'`",
    "lint": "eslint src/server/*.ts src/server/startupConfigs/*.ts src/frontend/*.ts --fix",
    "format": "prettier --write src/server/*.ts src/server/startupConfigs/*.ts src/frontend/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:pzinn/Macaulay2Web.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-flow": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-preval": "^5.0.0",
    "babel-plugin-version-inline": "^1.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.0",
    "less": "3.13.1",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.2.1",
    "raw-loader": "^4.0.2",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1"
  },
  "license": "MIT"
}
